pkgname=tailscale-pikvm
pkgver=0.1
pkgrel=1
pkgdesc="Pi-KVM fixes for Tailscale"
url="https://tailscale.com"
license=(GPL)
arch=(any)
depends=(tailscale)
source=(override.conf)
md5sums=(SKIP)


package() {
	mkdir -p "$pkgdir/etc/systemd/system/tailscaled.service.d"
	install -m644 override.conf "$pkgdir/etc/systemd/system/tailscaled.service.d/override.conf"
}
